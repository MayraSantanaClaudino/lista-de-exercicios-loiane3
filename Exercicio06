import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    
		//Scanner scan = new Scanner(System.in);
		
		for (int i = 1; i <= 20; i++){
		    System.out.println(i);
		}
		
		for (int i = 1; i <= 20; i++){
		    System.out.print(i + " ");
		}
		
	}
}
