//O maior número entre 5 números
//May Santana, 2023

import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    
		Scanner scan = new Scanner(System.in);
		
		int num;
		int maior = Integer.MIN_VALUE;
		
		for (int i = 0; i < 5; i++){
		    System.out.println("Informe um número:");
		    num = scan.nextInt();
		    
		    if (num > maior){
		        maior = num;
		    }
		    
		}
		
		System.out.println(maior);
		
	}
}
