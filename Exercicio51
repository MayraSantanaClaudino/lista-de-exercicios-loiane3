import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Informe um número");
		int n = scan.nextInt();
		double soma = 0;
		
		for (int i = 1; int j = 1; i <= n; i++; j += 2){
		    System.out.print(i + " / " + j + " + ...");
		    
		    soma += i / j;
		}
		
		System.out.println(\nsoma);
		
	}
}
