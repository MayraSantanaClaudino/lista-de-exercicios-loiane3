import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Informe um número");
		int n = scan.nextInt();
		
		double soma = 0;
		
		for (int i = 1; i <= n; i++){
		    soma += 1 / i;
		}
		
		System.out.println(soma);
		
	}
}
