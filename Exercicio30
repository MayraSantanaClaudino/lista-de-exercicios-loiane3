//Tabela de preços
//May Santana, 2023

import java.util.*;

public class Main
{
	public static void main(String[] args) {
		
		System.out.println("Preço do pão: R$0,18");
		System.out.println("Panificadora Pão de Ontem - Tabela de preços");
		
		for (int i = 1; i <= 50; i++){
		    System.out.println(i + " - R$ " + (0.18 * i));
		}
		
	}
}
